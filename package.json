    "scripts": {
        "myscripts": "yarn getjsons && yarn test -u && yarn lint:js && yarn lint:scss && git add --all",
        "start": "cp .env.dev .env && yarn getjsons && cross-env NODE_ENV=development webpack serve",
        "preNgrok": "cp .env.clavo .env && yarn getjsons && cross-env NODE_ENV=development webpack serve --disable-host-check",
        "build": "yarn getjsons && yarn build:clean && cross-env NODE_ENV=production webpack",
        "build:clean": "rimraf ./build",
        "test": "jest --env=jsdom --config jestconfig.json --silent",
        "testcover": "jest --env=jsdom --config jestconfig.json --coverage",
        "lint": "yarn lint:js && yarn lint:scss",
        "lint:js": "eslint './src/**/*.{js,jsx}' --fix",
        "getjsons": "node src/constants/allschemas/getjsons.js",
        "lint:scss": "stylelint 'src/**/*.scss' --custom-syntax postcss-scss --fix"
    },
    "lint-staged": {
        "src/**/*.{js,jsx}": [
            "yarn lint:js"
        ],
        "src/**/*.scss": [
            "yarn lint:scss"
        ]
    },
